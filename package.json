{
	"name": "@lostisworld/svelte-textcircle",
	"version": "0.0.1",
	"keywords": [
		"svelte",
		"svelte component",
		"svelte text circle",
		"svelte text circle component",
		"animation",
		"css",
		"css animation",
		"css text circle"
	],
	"author": "LoSti's World",
	"license": "MIT",
	"homepage": "https://github.com/LoStis-World/svelte-textcircle#readme",
	"bugs": {
		"url": "https://github.com/LoStis-World/svelte-textcircle/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LoStis-World/svelte-textcircle.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist/svelte-textcircle",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/svelte-textcircle/index.js",
	"types": "./dist/svelte-textcircle/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/svelte-textcircle/index.d.ts",
			"svelte": "./dist/svelte-textcircle/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
